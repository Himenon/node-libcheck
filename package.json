{
  "name": "libcheck",
  "version": "1.1.0",
  "description": "Check the health of npm libraries installed with yarn or npm.",
  "author": {
    "name": "Himenon",
    "url": "https://github.com/Himenon"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Himenon/node-libcheck.git"
  },
  "bugs": {
    "url": "https://github.com/Himenon/node-libcheck/issues"
  },
  "bin": {
    "libcheck": "./bin/libcheck.js"
  },
  "files": [
    "bin",
    "lib",
    "package.json",
    "README.md"
  ],
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node ./lib/index.js --input ./yarn.lock --output ./output/parsed.json",
    "ts": "ts-node -P ./tsconfig.json",
    "build": "tsc -b tsconfig.json",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "yarn-deduplicate --fail",
    "gen:html": "node ./lib/index.js --input yarn.lock --html docs/index.html"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/glob": "^7.1.1",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.5.0",
    "@types/prettier": "^1.19.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.0.0",
    "ts-node": "^8.6.2",
    "type-fest": "^0.9.0",
    "typescript": "^3.7.5",
    "yarn-deduplicate": "^1.1.1"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^4.1.0",
    "glob": "^7.1.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "yarn-lockfile": "^1.1.1"
  }
}
